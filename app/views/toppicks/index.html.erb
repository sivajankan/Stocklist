<h1>Listing toppicks</h1>

<table>
  <tr>
    <th>Symbol</th>
    <th>Lasttrade</th>
    <th>Pickeddate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @toppicks.each do |toppick| %>
  <tr>
    <td><%= toppick.symbol %></td>
    <td><%= toppick.lasttrade %></td>
    <td><%= toppick.pickeddate %></td>
    <td><%= link_to 'Show', toppick %></td>
    <td><%= link_to 'Edit', edit_toppick_path(toppick) %></td>
    <td><%= link_to 'Destroy', toppick, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Toppick', new_toppick_path %>
